{
  "version": 3,
  "configurePresets": [
    {
      "name": "default-release",
      "description": "Release build with default compiler",
      "generator": "Unix Makefiles",
      "binaryDir": "build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "default-debug",
      "description": "Debug build with default compiler",
      "generator": "Unix Makefiles",
      "binaryDir": "build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }

    }
    
  ]
}
